{% extends 'base.html.twig' %}

{% block title %}Modifier la mission{% endblock %}

{% block body %}

<h1>Modifier la mission</h1>
{{ form_start(form) }}

{{ form_row(form.codeName) }}
        {{ form_row(form.title) }}
        {{ form_row(form.description) }}
        {{ form_row(form.begin_at) }}
        {{ form_row(form.finish_at) }}
        {{ form_row(form.country) }}
        {{ form_row(form.target) }}
        {{ form_row(form.speciality) }}
        {{ form_row(form.type) }}
        {{ form_row(form.status) }}
        {{ form_row(form.hidingPlace) }}
        {{ form_row(form.agent) }}
        {{ form_row(form.contact) }}
<div class="my-3">
    <button type="submit" class="btn btn-sm btn-warning">MODIFIER</button>
</div>
{{ form_end(form) }}

{% endblock %}