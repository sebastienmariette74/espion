{% extends 'base.html.twig' %}

{% block title %}Modifier la planque{% endblock %}

{% block body %}

<h1>Modifier la planque</h1>

{{ form_start(form) }}
    {{ form_row(form.code) }}
    {{ form_row(form.address) }}
    {{ form_row(form.zipcode) }}
    {{ form_row(form.city) }}
    {{ form_row(form.country) }}
    {{ form_row(form.type) }}
    <div class="my-3">
        <button type="submit" class="btn btn-sm btn-warning">MODIFIER</button>
        <a href="{{ path('admin_hidingPlaces') }}" class="btn btn-sm btn-danger">ANNULER</a>

    </div>
{{ form_end(form) }}

{% endblock %}